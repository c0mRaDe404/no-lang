/*  10 million iterations took 45 seconds atmost in my lang
    and python took 51 seconds
    and C took just 9seconds just a piece of cake
*/


let i = 5;
let j = 0;

if(i==5) {
    print("Hello world");
    if(1>5){
        print("I am not supreme");
    }else{
        while(j<i){
            print("I am supreme");
            let j = j+1;
        }
    }
}else{ 
    if(i==4) {
        print("Bye world"); 
    }else{
        print("I am the world");
    }
}
